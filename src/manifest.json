{
  "description": "Adds a context menu entry to remove duplicate bookmarks within a folder",
  "manifest_version": 3,
  "author": "MrOtherGuy",
  "name": "BmBGone",
  "version": "0.1",
  "permissions":["bookmarks","storage","menus"],
  "background": {
    "scripts": ["background.js"]
  },
  "options_ui": {
    "page": "options/options.html"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "bmbgone@example.com",
      "strict_min_version": "112.0"
    }
  }
}